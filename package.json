{
  "license": "ISC",
  "main": "index.js",
  "version": "1.0.0",
  "name": "react-go-to",
  "description": "Quick react starter with predefined architecture guidlines",
  "scripts": {
    "eslint": "eslint --fix \"./src/**/*.{ts,tsx}\"",
    "serve": "npm run prepare && webpack-dev-server --config ./webpack/webpack.server.js",
    "stylelint": "stylelint --fix \"./src/**/*.scss\" && prettier --write \"./src/**/*.scss\"",
    "pre:start": "node ./scripts/update-module-paths.js && node ./scripts/ensure-configurations.js",
    "build": "npm run prepare && cross-env NODE_ENV=production webpack --mode=production --config ./webpack/webpack.prod.js"
  },
  "dependencies": {
    "circular-dependency-plugin": "^5.2.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.2.1",
    "prettier": "^2.1.2",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "webpack-simple-progress-plugin": "^0.0.4"
  },
  "devDependencies": {}
}
